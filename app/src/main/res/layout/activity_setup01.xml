<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".setup01">


    <TextView
        android:id="@+id/alarm_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/seekbar_alarm_volume"
        app:layout_constraintEnd_toEndOf="@+id/seekbar_alarm_volume"
        app:layout_constraintStart_toStartOf="@+id/seekbar_alarm_volume" />

    <TextView
        android:id="@+id/textViewTitle"
        android:layout_width="248dp"
        android:layout_height="32dp"
        android:text="現在の各音量と設定"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.034" />

    <Button
        android:id="@+id/BT_onryousettei"
        android:layout_width="199dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="現在の設定値で設定"
        app:layout_constraintStart_toEndOf="@+id/BT_returnMain"
        app:layout_constraintTop_toTopOf="@+id/BT_returnMain" />

    <TextView
        android:id="@+id/alarm_dial_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:text="ダイヤル音量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarm_volume_title" />

    <TextView
        android:id="@+id/music_volume_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:text="ミュージック音量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarm_dial_volume" />


    <TextView
        android:id="@+id/notice_volume_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:text="通知音量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/music_volume_title" />


    <TextView
        android:id="@+id/ring_volume_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:text="着信音量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notice_volume_title" />


    <TextView
        android:id="@+id/sysmesg_volume_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:text="システム音量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ring_volume_title" />


    <TextView
        android:id="@+id/voice_volume_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:text="通話音量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sysmesg_volume_title" />

    <TextView
        android:id="@+id/dial_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/seekbar_dial_volume"
        app:layout_constraintEnd_toEndOf="@+id/seekbar_dial_volume"
        app:layout_constraintStart_toStartOf="@+id/seekbar_dial_volume" />

    <TextView
        android:id="@+id/music_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/seekbar_music_volume"
        app:layout_constraintEnd_toEndOf="@+id/seekbar_music_volume"
        app:layout_constraintStart_toStartOf="@+id/seekbar_music_volume" />


    <TextView
        android:id="@+id/notice_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/seekbar_notice_volume"
        app:layout_constraintEnd_toEndOf="@+id/seekbar_notice_volume"
        app:layout_constraintStart_toStartOf="@+id/seekbar_notice_volume" />

    <TextView
        android:id="@+id/ring_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/seekbar_ring_volume"
        app:layout_constraintEnd_toEndOf="@+id/seekbar_ring_volume"
        app:layout_constraintStart_toStartOf="@+id/seekbar_ring_volume" />

    <TextView
        android:id="@+id/sysmesg_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/seekbar_sysmesg_volume"
        app:layout_constraintEnd_toEndOf="@+id/seekbar_sysmesg_volume"
        app:layout_constraintStart_toStartOf="@+id/seekbar_sysmesg_volume" />

    <TextView
        android:id="@+id/voice_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/seekbar_voice_volume"
        app:layout_constraintEnd_toEndOf="@+id/seekbar_voice_volume"
        app:layout_constraintStart_toStartOf="@+id/seekbar_voice_volume" />

    <TextView
        android:id="@+id/manner_mode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="32dp"
        android:text="マナーモードの判定"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/voice_volume_title" />

    <SeekBar
        android:id="@+id/seekbar_dial_volume"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginEnd="16dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/alarm_dial_volume" />

    <SeekBar
        android:id="@+id/seekbar_music_volume"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginEnd="16dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/music_volume_title" />

    <SeekBar
        android:id="@+id/seekbar_notice_volume"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginEnd="16dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/notice_volume_title" />

    <SeekBar
        android:id="@+id/seekbar_ring_volume"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginEnd="16dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ring_volume_title" />

    <SeekBar
        android:id="@+id/seekbar_sysmesg_volume"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginEnd="16dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sysmesg_volume_title" />

    <SeekBar
        android:id="@+id/seekbar_voice_volume"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginEnd="16dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/voice_volume_title" />

    <Button
        android:id="@+id/BT_returnMain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:text="戻る"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/manner_mode" />

    <TextView
        android:id="@+id/alarm_volume_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="32dp"
        android:text="アラーム音量"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTitle" />

    <SeekBar
        android:id="@+id/seekbar_alarm_volume"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/alarm_volume_title" />


</android.support.constraint.ConstraintLayout>